{
  "name": "clickup-action",
  "description": "GitHub action wrapping the ClickUp API",
  "version": "0.0.1",
  "author": "Thibaut Tiberghien <thibaut@smplrspace.com>",
  "private": true,
  "homepage": "https://github.com/smplrspace/clickup-action",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smplrspace/clickup-action.git"
  },
  "bugs": {
    "url": "https://github.com/smplrspace/clickup-action/issues"
  },
  "keywords": [
    "actions",
    "clickup",
    "api"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
